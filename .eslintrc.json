{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "globals": {
    "window": false,
    "document": false,
    "location": false,
    "setTimeout": false,
    "clearTimeout": false,
    "localStorage": false,
    "jQuery": false
  },
  "rules": {
    "no-extra-parens": 1,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "func-call-spacing": [2, "never"],
    "no-unneeded-ternary": 2,
    "semi": 2,
    "quotes": [2, "single"],
    "no-var": 2,
    "indent": [2, 2],
    "space-in-parens": [2, "never"],
    "no-console": 2,
    "comma-spacing": 2,
    "computed-property-spacing": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "no-empty": [2, {"allowEmptyCatch": true}]
  },
  "env": {
    "es6": true
  }
}
